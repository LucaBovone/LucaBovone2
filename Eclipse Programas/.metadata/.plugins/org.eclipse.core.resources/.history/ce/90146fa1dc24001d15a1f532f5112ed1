#include <stdio.h>
#include <stdlib.h>

int main(void) {
	char caracter1;
	char caracter2;
	char caracter3;
	char caracter4;
	char caracter5;
	setbuf(stdout,NULL);


		printf("Ingrese Caracteres\n");
		scanf("%c %c %c %c %c ",&caracter1,&caracter2,&caracter3,&caracter4,&caracter5);





	return EXIT_SUCCESS;
}

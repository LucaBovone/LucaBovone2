#include <stdio.h>
#include <stdlib.h>

int main(void) {

	setbuf(stdout,NULL);
	int i;
	int numero;
	int promedio;


	for(i=0;i<10;i++){
		do{
		printf("Ingrese numero %d \n",i+1);
		scanf("%d",&numero);
		}
		while(numero<=0);


	}


	return EXIT_SUCCESS;
}
